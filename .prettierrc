{
  "semi": false,
  "singleQuote": true,
  "trailingComma": "es5",
  "printWidth": 80,
  "plugins": ["@ianvs/prettier-plugin-sort-imports"],
  "importOrder": [
    ".*styles.css$",
    "",
    "dayjs",
    "^react$",
    "^next$",
    "^next/.*$",
    "<BUILTIN_MODULES>",
    "<THIRD_PARTY_MODULES>",
    "^mantine/(.*)$",
    "^mantinex/(.*)$",
    "^mantine-tests/(.*)$",
    "^docs/(.*)$",
    "^/.*$",
    "^../(?!.*.css$).*$",
    "^./(?!.*.css$).*$",
    "\\.css$"
  ]
}
